include "/usr/share/themes/Raleigh/gtk-2.0/gtkrc"

style "user-font" {
	font_name = "Fira Sans Semi-Light 10.5"
}

widget_class "*" style "user-font"

gtk-font-name="Fira Sans Semi-Light 10.5"

# include "~/.gtkrc.mine"

gtk-xft-dpi       = 227
gtk-xft-antialias = 1
gtk-xft-hinting   = 1
gtk-xft-hintstyle = hintfull
gtk-xft-rgba      = rgb
